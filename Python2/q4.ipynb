     "text": [
      "Enter the number: 153\n",
      "153  is an Armstrong number\n"
     ]
    }
   ],
   "source": [
    "num = int(input('Enter the number: '))\n",
    "order = len(str(num))\n",
    "sum = 0\n",
    "temp = num\n",
    "while temp > 0:\n",
    "    digit = temp % 10\n",
    "    sum += digit ** order\n",
    "    temp //= 10\n",
    "    \n",
    "if num == sum:\n",
    "    print(num, ' is an Armstrong number')\n",
    "else:\n",
    "    print(num, ' is not an Armstrong number')"
   ]
  },
