   "source": [
    "num = int(input('Enter the number: '))\n",
    "order = len(str(num))\n",
    "sum\n",
    "temp = num\n",
    "while temp > 0:\n",
    "    digit = temp % 10\n",
    "    sum += digit ** order\n",
    "    temp //= 10\n",
